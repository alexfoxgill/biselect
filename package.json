{
  "name": "biselect",
  "version": "2.0.2",
  "description": "Two-way selectors for Redux-like applications, TypeScript-first.",
  "main": "dist/index",
  "typings": "dist/index",
  "watch": {
    "test": "src/**/*.ts"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "start": "webpack-cli -w --config webpack.conf.js",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test-watch": "npm-watch test"
  },
  "bugs": {
    "url": "http://github.com/alexfoxgill/biselect/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/alexfoxgill/biselect.git"
  },
  "keywords": [
    "redux",
    "selector",
    "selectors",
    "lens",
    "lenses",
    "optics"
  ],
  "author": "Alex Fox Gill <email@alexfoxgill.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "npm-watch": "^0.3.0",
    "source-map-loader": "^0.2.3",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ]
}
